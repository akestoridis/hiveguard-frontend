  "bugs": {
    "url": "https://github.com/akestoridis/hiveguard-frontend/issues"
  },
  "dependencies": {
    "bootstrap": "^4.5.3",
    "react": "^17.0.1",
    "react-bootstrap": "^1.4.3",
    "react-dom": "^17.0.1",
    "react-router-bootstrap": "^0.25.0",
    "react-router-dom": "^5.2.0",
    "react-router-hash-link": "^2.3.1"
